TARGET := test2
SRC := $(TARGET).c
OBJ := $(TARGET).o


CFLAGS := -g -Wall
LFLAGS := -lm -pthread
CC := gcc $(CFLAGS) $(LFLAGS)


all : $(TARGET)


$(TARGET): $(OBJ)
	$(CC) -o $(TARGET) $(TARGET).c -L. -ldlink
	export LD_LIBRARY_PATH="/home/meit2308/my_cpi/cpi2308"
	echo $LD_LIBRARY_PATH
.c.o:
	$(CC) -c $^
clean:
	rm -f $(TARGET) $(OBJ)
